<div class="image-row-wrap {{rowClass}}">
	{{#if heading}}<p class="emphasized -section">{{heading}}</p>{{/if}}
	<div class="image-row">
		{{#each imgs}}
	    <div class="img-wrap {{imgClass}}">
	        {{#if caption}}<p class="emphasized">{{caption}}</p>{{/if}}
	        {{{img}}}
	    </div>
	  {{/each}}
	</div>
</div>